# Django Core Settings
# DEBUGモード（本番環境では必ずFALSEに設定してください）
DEBUG=TRUE

# Djangoのシークレットキー（本番環境では必ず独自の値に変更してください）
# 生成方法: python -c 'from django.core.management.utils import get_random_secret_key; print(get_random_secret_key())'
SECRET_KEY=your-secret-key-here

# アクセスを許可するホスト（カンマ区切り）
ALLOWED_HOSTS=localhost,127.0.0.1

# Database Settings for Django
POSTGRES_ENGINE=your.django.db.backends.postgresql
POSTGRES_DB=your_db
POSTGRES_USER=you
POSTGRES_PASSWORD=your-database-password-here
POSTGRES_HOST=db
POSTGRES_PORT=5432

# Gemini
GEMINI_API_KEY=your_Gemini_API_key